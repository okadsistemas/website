          "_customCSS": "/*\n * VARS\n */\n@hidePrices: true;\n@planColors: @primaryColor, cornflowerblue, #ffff62, mediumpurple;\n/*\n * MIXINS\n */\n.planColors (@index) when (@index > 0){\n    @textColor: contrast(extract(@planColors, @index), lighten(black, 12%), darken(white, 12%));\n    div:nth-child(@{index}) > .mbr-plan > .mbr-plan-header {\n        background-color: extract(@planColors, @index) !important;\n        h3.mbr-plan-title,\n        small.mbr-plan-subtitle, \n        small.mbr-plan-price-desc{\n            color: @textColor;   \n        }\n    }\n    .planColors(@index - 1);\n}\n/*\n * STYLE\n */\n.mbr-price-table > .row {\n    .planColors(length(@planColors));\n        small.mbr-price-term {\n        font-size: 1rem;\n    }\n    .mbr-plan{\n        .mbr-plan-title {\n            font-size: 2rem;\n        }\n        .list-group-item {\n            line-height: inherit;\n            padding-top: 1rem;\n            padding-bottom: 1rem;\n        }\n        .mbr-plan-header{\n            & when (@hidePrices){\n                padding: 2rem !important;\n            }\n            .card-title{\n                & when (@hidePrices){\n                    margin: 0 !important;\n                }\n            }\n            .mbr-price, .mbr-plan-price-desc{\n                & when (@hidePrices){\n                    display: none;\n                }\n            }\n        }\n        .mbr-plan-body {\n            padding-top: 0;\n            .mbr-plan-list {\n                padding-bottom: 1rem;\n                padding-top: 0;\n            }\n            .mbr-plan-btn > a.btn {\n                padding: .7rem;\n            }\n        }\n    }\n}\n\n"
        },
